function modify$!{domainClassNameEN}() {
	var param = {};
#foreach(${basicColumn} in ${basicColumnList})
	param.$!{basicColumn.columnClassName} = $("#$!{basicColumn.columnClassName}").val();
#end
	//jQuery.blockUI({message: "正在查询数据，请稍候..."});
	//jQuery.ajax()

	jQuery.post(
		'/$!{domainPropertyNameEN}/modify$!{domainClassNameEN}',
		{
			'$!{domainPropertyNameEN}':param,
			'$!{pkColumn.columnClassName}':$("#$!{pkColumn.columnClassName}").val()
		},
		function (result) {
			//jQuery.unblockUI();
			if (result.code == 200) {
				alert("成功");
			} else {
				alert("错误！" + result.message);
			}
		},
		"json"
	);
}
